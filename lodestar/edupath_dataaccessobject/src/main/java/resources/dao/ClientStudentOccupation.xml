<mapper namespace="ClientStudentOccupation">

    <parameterMap id="paramMap" type="com.lodestar.edupath.datatransferobject.dto.APIS.client.ClientStudentOccupationDTO"></parameterMap>
	<resultMap id="SelectAllResult" type="com.lodestar.edupath.datatransferobject.dto.APIS.client.ClientStudentOccupationDTO"></resultMap>
	
	<select id="getPersonalityCodeOccupationByStudentId" parameterMap="paramMap" resultMap="SelectAllResult">
	    SELECT studentId,personalityCode,occList FROM ClientStudentOccupation WHERE studentId = #{studentId}
	</select>

