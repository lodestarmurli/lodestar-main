	<select id="getClientStudentOccupation" parameterType="java.util.Map" resultMap="SelectAllResult">
	  SELECT * From ClientStudentOccupation 
      where token1 = #{token} or  token2 = #{token}
	</select>	
	
	
	<insert id="insertorUpdateStudentOccupation" parameterMap="paramMap" >
	   INSERT INTO ClientStudentOccupation (studentId, occ1Id, occ2Id, pdfToken, token1, token2, occList, personalityCode) VALUES (#{studentId}, #{occ1Id}, #{occ2Id}, #{pdfToken}, #{token1}, #{token2}, #{occList}, #{personalityCode})
	   ON DUPLICATE KEY UPDATE	studentId=#{studentId}, occ1Id=#{occ1Id}, occ2Id=#{occ2Id}, pdfToken=#{pdfToken}, token1=#{token1}, token2=#{token2}, occList=#{occList}, personalityCode=#{personalityCode}
	</insert>
